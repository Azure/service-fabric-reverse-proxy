{
    "listeners": [
        {
            "address": "tcp://0.0.0.0:19081",
            "filters": [
                {
                    "name": "http_connection_manager",
                    "config": {
                        "access_log": [
                            {
                                "path": "./log/sfreverseproxy.access_log.log"
                            }
                        ],
                        "codec_type": "auto",
                        "stat_prefix": "reverse_proxy_http",
                        "rds": {
                            "cluster": "rds",
                            "route_config_name": "reverse_proxy",
                            "refresh_delay_ms": 10000
                        },
                        "filters": [
                            {
                                "type": "decoder",
                                "name": "router",
                                "config": {}
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "admin": {
        "access_log_path": "./log/sfreverseproxy.access_log.admin.log",
        "address": "tcp://0.0.0.0:8001"
    },
    "cluster_manager": {
        "sds": {
            "cluster": {
                "name": "sds",
                "type": "static",
                "connect_timeout_ms": 5000,
                "lb_type": "round_robin",
                "hosts": [
                    {
                        "url": "tcp://127.0.0.1:5000"
                    }
                ]
            },
            "refresh_delay_ms": 10000
        },
        "clusters": [
            {
                "name": "rds",
                "type": "static",
                "connect_timeout_ms": 5000,
                "lb_type": "round_robin",
                "hosts": [
                    {
                        "url": "tcp://127.0.0.1:5000"
                    }
                ]
            }
        ],
        "cds": {
            "cluster": {
                "name": "cds",
                "connect_timeout_ms": 5000,
                "lb_type": "round_robin",
                "type": "static",
                "hosts": [
                    {
                        "url": "tcp://127.0.0.1:5000"
                    }
                ]
            },
            "refresh_delay_ms": 10000
        }
    }
}